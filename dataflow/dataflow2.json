{
	"name": "dataflow2",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"linkedService": {
						"referenceName": "firstDatalake",
						"type": "LinkedServiceReference"
					},
					"name": "DimAirline"
				},
				{
					"linkedService": {
						"referenceName": "firstDatalake",
						"type": "LinkedServiceReference"
					},
					"name": "DimFlight",
					"description": "Add source dataset"
				},
				{
					"linkedService": {
						"referenceName": "firstDatalake",
						"type": "LinkedServiceReference"
					},
					"name": "DimFactBookings"
				}
			],
			"sinks": [
				{
					"linkedService": {
						"referenceName": "firstDatalake",
						"type": "LinkedServiceReference"
					},
					"name": "GoldSink"
				}
			],
			"transformations": [
				{
					"name": "Airlinejoin"
				},
				{
					"name": "selectColumns"
				},
				{
					"name": "aggregateAirline"
				},
				{
					"name": "Ranking"
				},
				{
					"name": "filterTops"
				},
				{
					"name": "Insert"
				}
			],
			"scriptLines": [
				"source(output(",
				"          airline_id as short,",
				"          airline_name as string,",
				"          country as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'delta',",
				"     fileSystem: 'silver',",
				"     folderPath: 'dimAirline') ~> DimAirline",
				"source(output(",
				"          flight_id as short,",
				"          flight_number as string,",
				"          departure_timeTemp as string,",
				"          arrival_time as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'delta',",
				"     fileSystem: 'silver',",
				"     folderPath: 'dimFlight') ~> DimFlight",
				"source(output(",
				"          booking_id as integer,",
				"          passenger_id as integer,",
				"          flight_id as integer,",
				"          airline_id as integer,",
				"          origin_airport_id as integer,",
				"          destination_airport_id as integer,",
				"          booking_date as date,",
				"          ticket_cost as integer,",
				"          flight_duration_mins as integer,",
				"          checkin_status as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'delta',",
				"     fileSystem: 'silver',",
				"     folderPath: 'Dimbookingsql') ~> DimFactBookings",
				"DimFactBookings, DimAirline join(DimFactBookings@airline_id == DimAirline@airline_id,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> Airlinejoin",
				"Airlinejoin select(mapColumn(",
				"          booking_id,",
				"          passenger_id,",
				"          flight_id,",
				"          airline_id = DimFactBookings@airline_id,",
				"          origin_airport_id,",
				"          destination_airport_id,",
				"          booking_date,",
				"          ticket_cost,",
				"          flight_duration_mins,",
				"          checkin_status,",
				"          airline_name,",
				"          country",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> selectColumns",
				"selectColumns aggregate(groupBy(airline_name),",
				"     TotalSales = sum(ticket_cost)) ~> aggregateAirline",
				"aggregateAirline window(asc(TotalSales, true),",
				"     Top = denseRank()) ~> Ranking",
				"Ranking filter(Top <6) ~> filterTops",
				"filterTops alterRow(insertIf(1==1)) ~> Insert",
				"Insert sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'delta',",
				"     fileSystem: 'gold',",
				"     folderPath: 'BussinesView',",
				"     overwrite: true,",
				"     mergeSchema: false,",
				"     autoCompact: false,",
				"     optimizedWrite: false,",
				"     vacuum: 0,",
				"     deletable: false,",
				"     insertable: true,",
				"     updateable: false,",
				"     upsertable: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> GoldSink"
			]
		}
	}
}